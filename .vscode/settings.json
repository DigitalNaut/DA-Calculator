{
  "cSpell.words": [
    "clsx",
    "fontawesome",
    "fortawesome",
    "svgr",
    "tailwindcss",
    "tseslint",
    "typecheck"
  ],
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "": "",
    "*.ts": "${capture}.js, ${capture}.test.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts, ${capture}.test.js"
  }
}